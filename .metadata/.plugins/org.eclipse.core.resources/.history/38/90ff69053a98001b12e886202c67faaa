package DecoratoriFigure;

import Figure.Figura;

public class ColoreDecorator extends DecoratorFigura {
	
	private String colore;

	public ColoreDecorator(Figura figura, String colore) {
		super(figura);
		this.colore = colore;
	}

	@Override
	public void stampaPersonalizzata() {
		System.out.println("");
	}
	
}
