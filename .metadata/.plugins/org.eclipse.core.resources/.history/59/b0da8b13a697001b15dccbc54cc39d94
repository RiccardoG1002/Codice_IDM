import Vagoni.Treno;
import Vagoni.Vagone;

public abstract class FabbricaTreni {
	 //private static Treno treno = null;
	 
	 public static final Treno ordinaTreno(String ordine) {
		 Treno t = null;
		 if(controllo(ordine)) {
			 t = new Treno();
			 
		 }
		 return t;
	 }
	 
	 private static final boolean controllo(String ordine) {
		 return true;
	 }
	 
	 public abstract Vagone crea();
}
