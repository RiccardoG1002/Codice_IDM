package DecoratoriFigure;

import Figure.Figura;

public abstract class DecoratorFigura implements Figura {
	
	private Figura figura;
	
	public final void stampa() {
		figura.stampaPersonalizzata();
	}

	@Override
	public final double area() {
		return figura.area();
	}

	public abstract void stampaPersonalizzata();
}
