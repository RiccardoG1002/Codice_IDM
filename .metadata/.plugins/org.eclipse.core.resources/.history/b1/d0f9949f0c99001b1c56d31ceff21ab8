package Figure;

import java.util.Comparator;

public class ComparatoreFigure implements Comparator<Figura> {

	@Override
	public final int compare(Figura o1, Figura o2) {
		if(o1.getClass().getSimpleName().equals(o2.getClass().getSimpleName())) return 0;
		else if(o1.getClass().getSimpleName().compareTo(o2.getClass().getSimpleName())) return 1;
		else return -1;
		//return o1.area() - o2.area();
	}

}