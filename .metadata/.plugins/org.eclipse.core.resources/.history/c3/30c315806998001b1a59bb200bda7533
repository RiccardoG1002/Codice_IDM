import Vagoni.Treno;

public class TestTreni {

	public static void main(String[] args) {
		
		Treno treno = Venditore.ordinaTreno("HCCCCCRP");
		treno.accendi();
		treno = Venditore.ordinaTreno("HCRRPPCCRP");
		treno.accendi();
		treno = Venditore.ordinaTreno("HCCCCCCCC");
		treno.accendi();
		
	}

}
