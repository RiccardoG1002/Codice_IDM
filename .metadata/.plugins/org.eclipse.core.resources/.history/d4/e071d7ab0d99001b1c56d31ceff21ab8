package Figure.PoligoniRegolari;

import Figure.Figura;

public class PoligonoRegolareBase implements PoligonoRegolare {

	@Override
	public int compareTo(Figura o) {
		if(this.area() == o.area()) return 0;
		else if(this.area() > o.area()) return 1;
		else return -1;
	}

	@Override
	public double area() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public int numeroLati() {
		// TODO Auto-generated method stub
		return 0;
	}

}
