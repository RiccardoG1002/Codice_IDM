package NegozioTreni;

import Treni.Treno;

public abstract class NegozioTreni {
	
	public final Treno ordinaTreno() {
		return null;
	}
	
	public final Treno aggiungiCarrozza() {
		return null;
	}
	
	private boolean controllaOrdine() {
		return true;
	}
	
	private boolean controllaAggiunta() {
		return true;
	}
	
	public abstract Treno ordinaTrenoPersonalizzata();
	
	public abstract Treno aggiungiCarrozzaPersonalizzata();

}
