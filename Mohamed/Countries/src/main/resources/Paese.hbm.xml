<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.corso.paesi.Paese" table = "standard">
      
      <!-- ID -->
      <id name = "codice" type = "string" column = "codice">
         <generator class="assigned"/>
      </id>
      
      <!-- PROPERTY -->
      <property name = "nome" column = "nome"  type = "string"/>
      
      
      <!-- RELATIONS -->
      
       <set name="sinonimi" inverse="true" lazy="false" cascade="all">
            <key column="codice" not-null="true" />
            <one-to-many class="com.corso.paesi.Sinonimo"/>
       </set>
       

      
   </class>
</hibernate-mapping>
